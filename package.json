{
  "name": "commute-time-collector",
  "version": "0.0.1",
  "description": "A service that collects time to get from point A to point B via Yandex maps",
  "scripts": {
    "start": "NODE_ENV=dev ts-node ./index.ts",
    "start:prod": "NODE_ENV=prod ts-node ./index.ts",
    "tsc": "tsc",
    "lint": "eslint --ext .tsx --ext .ts --format codeframe './'",
    "lint:fix": "eslint --debug --fix",
    "kill-chromedriver": "pkill chromedriver"
  },
  "author": "Yury Baykov",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:OkciD/commute-time-collector.git"
  },
  "private": true,
  "dependencies": {
    "@types/chromedriver": "2.38.0",
    "@types/flat": "0.0.28",
    "@types/minimist": "1.2.0",
    "@types/node": "13.1.8",
    "@types/request-promise-native": "1.0.17",
    "chalk": "*",
    "chromedriver": "79.0.0",
    "csv-stringify": "5.3.6",
    "fecha": "*",
    "flat": "5.0.0",
    "minimist": "1.2.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "tough-cookie": "*",
    "ts-node": "8.6.2",
    "typescript": "3.7.5",
    "webdriver": "*",
    "webdriverio": "5.18.6",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "6.3.1",
    "eslint-plugin-import": "2.20.0",
    "husky": "4.0.10",
    "lint-staged": "10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "logsDir": "./logs"
}
