{
  "name": "commute-time-collector",
  "version": "0.0.1",
  "description": "A service that collects time to get from point A to point B via Yandex maps",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "NODE_ENV=dev ts-node src/index.ts",
    "tsc": "tsc",
    "lint": "eslint --ext .tsx --ext .ts --format codeframe './'",
    "lint:fix": "eslint --debug --fix",
    "test": "jest"
  },
  "author": "Yury Baykov",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:OkciD/commute-time-collector.git"
  },
  "private": true,
  "dependencies": {
    "@types/cheerio": "0.22.17",
    "@types/flat": "5.0.0",
    "@types/minimist": "1.2.0",
    "@types/node": "12.12.28",
    "@types/node-cron": "2.0.3",
    "@types/request": "2.48.4",
    "@types/user-agents": "1.0.0",
    "chalk": "*",
    "cheerio": "1.0.0-rc.3",
    "csv-stringify": "5.3.6",
    "fecha": "*",
    "flat": "5.0.0",
    "minimist": "1.2.5",
    "node-cron": "2.0.3",
    "request": "*",
    "tor-request": "3.1.0",
    "tough-cookie": "*",
    "ts-node": "8.8.2",
    "type-fest": "0.13.1",
    "typescript": "3.8.3",
    "user-agents": "1.0.559",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "7.2.1",
    "eslint-plugin-import": "2.20.2",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.3",
    "ts-jest": "25.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:fix",
      "git add"
    ]
  }
